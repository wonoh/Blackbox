<DOCTYPE html>
<html>
 <head>
   <title>BLACKBOX</title>
   <meta charset="utf-8">
   <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
   <meta name="description" content="">
   <meta name="author" content="">
   <link href="../stylesheets/bootstrap.min.css" rel="stylesheet">
   <link href="../stylesheets/font-awesome.min.css" rel="stylesheet" type="text/css">
   <link href="https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800" rel="stylesheet" type="text/css">
   <link href="https://fonts.googleapis.com/css?family=Merriweather:400,300,300italic,400italic,700,700italic,900,900italic" rel="stylesheet" type="text/css">
   <link href="../stylesheets/magnific-popup.css" rel="stylesheet">
   <link href="../stylesheets/creative.min.css" rel="stylesheet">
 </head>
 <body id="page-top">
     <nav id="mainNav" class="navbar navbar-expand-lg navbar-light fixed-top">
         <div class="container">
              <a class="carousel-control-prev" href="#" onClick="history.back()" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
              </a>

             <a href="/" class="navbar-brand js-scroll-trigger">BLACKBOX</a>
             <button type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation" class="navbar-toggler navbar-toggler-right">
                 <span class="navbar-toggler-icon"></span>
             </button>
             <div id="navbarResponsive" class="collapse navbar-collapse">
                 <ul class="navbar-nav ml-auto">
                     <% if(id) { %>
                        <li class="nav-item">
                          <a href="/logout" class="nav-link js-scroll-trigger">logout</a>
                        </li>
                        <% }else{ %>
                        <li class="nav-item">
                          <a href="/login" class="nav-link js-scroll-trigger">login</a>
                        </li>
                        <% } %>
                     <li class="nav-item">
                         <a href="/consult" class="nav-link js-scroll-trigger">Consult</a>
                     </li>
                     <li class="nav-item">
                         <a href="/eyewitness" class="nav-link js-scroll-trigger">Eyewitness</a>
                     </li>
                     <li class="nav-item">
                         <a href="/setting" class="nav-link js-scroll-trigger">Setting</a>
                     </li>
                 </ul>
             </div>
         </div>
     </nav>
     <section id="client" style="width:100%;" class="bg-dark text-white">
         <div class="container">
             <h2 style="font-family:Nanum Gothic;" class="section-heading text-white" color="#FF8000">
                 <storng>CASE REQUEST | 사건 의뢰</storng>
                 <form action="/admin_setting/c_user_info" method="GET">
                   <button type="submit" style="margin-right:200px;background-color:#ffffff;border-radius:4px;color:#000000;">의뢰인 사용자 보기</button>
                 </form>
                 <form action="/admin_setting/l_user_info" method="GET">
                   <button type="submit" style="margin-left:200px;background-color:#ffffff;border-radius:4px;color:#000000;">변호사 사용자 보기</button>
                 </form>
             <hr class="light my-4">
             <table align="left" width="700" border="1">
               <tr bgcolor="#BDBDBD" height="100" align="center">
                 <th>이름</th><th>아이디</th><th>E-Mail</th><th>패스워드</th><th>전화번호</th><th>휴대전화</th><th>삭제</th>
               </tr>
              <%data.forEach(function(item,index){%>
               <tr align="center"  bgcolor="#FFFFFF">
                 <td><%=item.name%></td> <td><%=item.userid%></td><td><%=item.email%></td><td><%=item.password%></td><td><%=item.tele%></td><td><%=item.phone%></td><td>여기삭제박스</td>
                 <td>
                   <a href="/consult/request_content?num=<%=data.num%>">삭제하기</a>
                 </td>
               </tr>
              <%})%>
             </table>
             <div class="form-group col-lg-8">
               <form action='/consult/request_insert' method='GET'>
                 <button type="submit" style="margin-left:480px;background-color:#f05f40;border-radius:4px;color:#ffffff;" class="btn btn-light btn-xl sr-button">의뢰하기</button>
               </form>
             </div>
           </div>
         </section>
         <script src="../javascripts/jquery.min.js"></script>
         <script src="../javascripts/bootstrap.bundle.min.js"></script>
         <script src="../javascripts/jquery.easing.min.js"></script>
         <script src="../javascripts/scrollreveal.min.js"></script>
         <script src="../javascripts/jquery.magnific-popup.min.js"></script>
         <script src="../javascripts/creative.min.js"></script>
       </body>
</html>
