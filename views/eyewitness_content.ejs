<!DOCTYPE html>
<html>

    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta name="description" content="">
        <meta name="author" content="">
        <title>목격 제보</title>
        <!-- Bootstrap core CSS-->
        <link href="../stylesheets/bootstrap.min.css" rel="stylesheet">
        <!-- Custom fonts for this template-->
        <link href="../stylesheets/font-awesome.min.css" rel="stylesheet" type="text/css">
        <link href="https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800" rel="stylesheet" type="text/css">
        <link href="https://fonts.googleapis.com/css?family=Merriweather:400,300,300italic,400italic,700,700italic,900,900italic" rel="stylesheet" type="text/css">
        <!-- Plugin CSS-->
        <link href="../stylesheets/magnific-popup.css" rel="stylesheet">
        <!-- Custom styles for this template-->
        <link href="../stylesheets/creative.min.css" rel="stylesheet">
    </head>

    <body id="page-top" style="background-image:url('../img/eyewitness-image.jpg');background-size:100% 140%">
        <!-- Navigation-->
        <nav id="mainNav" class="navbar navbar-expand-lg navbar-light fixed-top">
            <div class="container">
              <a class="carousel-control-prev" href="#" onClick="history.back()" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
              </a>

                <a href="/" class="navbar-brand js-scroll-trigger">BLACKBOX</a>
                <button type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation" class="navbar-toggler navbar-toggler-right">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div id="navbarResponsive" class="collapse navbar-collapse">
                    <ul class="navbar-nav ml-auto">
                        <% if(id) { %>
                        <li class="nav-item">
                          <a href="/logout" class="nav-link js-scroll-trigger">logout</a>
                        </li>
                        <% }else{ %>
                        <li class="nav-item">
                          <a href="/login" class="nav-link js-scroll-trigger">login</a>
                        </li>
                        <% } %>
                        <li class="nav-item">
                            <a href="/consult" class="nav-link js-scroll-trigger">Consult</a>
                        </li>
                        <li class="nav-item">
                            <a href="/eyewitness" class="nav-link js-scroll-trigger">Eyewitness</a>
                        </li>
                        <li class="nav-item">
                            <a href="/setting" class="nav-link js-scroll-trigger">Setting</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <section id="services" class="text-white">
            <div class="container">
                <div class="col-lg-12 text-center">
                    <br>
                    <br>
                    <h2 font-family:nanum="" class="section-heading style=">
                        <strong>EYEWITNESS</strong>
                    </h2>
                    <hr class="my-4">
                    <br>
                </div>
                <section id="service" class ="text-white">
                  <div class="row">
                  <div class="col-xs-12 col-md-10 text-center" style="margin-left:50px;margin-top:50px;">
                      <table class="table table-bordered">
                          <caption></caption>
                          <colgroup></colgroup>
                          <tbody>
                          <%data.forEach(function(item,index){%>
	                           <tr style="text-align:center;margin-bottom:30px;">
                                <th bgcolor="#BDBDBD">
                                    <label style="list-style-type:none;font-family:Nanum Gothic;">사건 날짜</label>
                                </th>
                                <th bgcolor="#BDBDBD">
                                  <label style="list-style-type:none;font-family:Nanum Gothic;">제목</label>
                                </th>
                                <th bgcolor="#BDBDBD">
                                  <label style="list-style-type:none;font-family:Nanum Gothic;">의뢰인</label>
                                </th>
                              </tr>
                              <tr style="text-align:center">
                                <td bgcolor="#ffffff" class="text-dark">
                                  <h5><%=item.date%></h5>
                                </td>
                                <td bgcolor="#ffffff" class="text-dark">
                                  <h5><%=item.title%></h5>
                                </td>
                                <td bgcolor="#ffffff" class="text-dark">
                                  <h5><%=item.userid%></h5>
                                </td>
                              </tr>
                              <tr style="text-align:center;">
                                <th colspan="3" bgcolor="#BDBDBD">
                                    <label style="font-family:Nanum Gothic;">내용</label>
                                </th>
                              </tr>
                              <tr>
                                <td colspan="3" bgcolor="#ffffff" class="text-dark">
                                    <h5><%=item.content%></h5>
                                </td>
                              </tr>
                              <tr style="text-align:center">
                                <th bgcolor="#BDBDBD">
                                  <label style="list-style-type:none;font-family:Nanum Gothic;">전화 번호</label>
                                </th>
                                <td bgcolor="#ffffff" class="text-dark">
                                  <h5><%=phone%></h5>
                                </td>
                              </tr>
                        </tbody>
                    </table>
                    <% }); %>
            </div>
        </section>
        <script src="../javascripts/jquery.min.js"></script>
        <script src="../javascripts/bootstrap.bundle.min.js"></script>
        <script src="../javascripts/jquery.easing.min.js"></script>
        <script src="../javascripts/scrollreveal.min.js"></script>
        <script src="../javascripts/jquery.magnific-popup.min.js"></script>
        <script src="../javascripts/creative.min.js"></script>
    </body>

</html>
